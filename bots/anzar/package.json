  "description": "AI-powered chatbot for ROFLFaucet chat system with multi-service AI routing",
  "main": "start-roflbot.js",
  "scripts": {
    "start": "node start-roflbot.js",
    "dev": "NODE_ENV=development node start-roflbot.js",
    "test": "jest",
    "lint": "eslint *.js",
